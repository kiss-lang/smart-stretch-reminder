{
	"main": "bin/extension.js",
	"name": "smart-stretch-reminder",
	"description": "",
	"repository": {
		"url": "",
		"type:": "git"
	},
	"homepage": "",
	"categories": [],
	"extensionPack": [],
	"publisher": "kiss-lang",
	"contributes": {
		"keybindings": [],
		"commands": [
			{
				"title": "smart-stretch-reminder: Test smart-stretch-reminder stretch message",
				"command": "smart-stretch-reminder.testShowStretchMessage"
			}
		],
		"configuration": {
			"title": "smart-stretch-reminder",
			"properties": {
				"smart-stretch-reminder.secondsAfterLastEdit": {
					"type": "number",
					"default": 5,
					"minimum": 0
				},
				"smart-stretch-reminder.minutesOfStretching": {
					"type": "number",
					"default": 3,
					"minimum": 0
				},
				"smart-stretch-reminder.stretchMessage": {
					"type": "string",
					"default": "Now it's time to stretch! Take this seriously!"
				},
				"smart-stretch-reminder.minutesBetweenStretching": {
					"type": "number",
					"default": 20,
					"minimum": 0
				}
			}
		}
	},
	"engines": {
		"vscode": "^1.4.0"
	},
	"devDependencies": {
		"vsce": "^2.15.0"
	},
	"version": "0.1.0",
	"activationEvents": [
		"onStartupFinished"
	],
	"tags": [],
	"displayName": "Smart Stretch Reminder"
}
